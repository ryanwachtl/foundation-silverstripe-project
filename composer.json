{
	"name": "rywa/silverstripe-init",
	"description": "A Composer-based SilverStripe Framework and CMS Project",
	"require": {
		"php": ">=5.3.2",
		"silverstripe/framework": "3.1.x@stable",
		"silverstripe/cms": "3.1.x@stable",
		"rywa/silverstripe-foundation": "~1.0",
		"rywa/silverstripe-foundation-forms": "dev-master",
		"silverstripe/userforms": "dev-master",
		"silverstripe/googlesitemaps": "dev-master",
		"unclecheese/betterbuttons": "dev-master",
		"ajshort/silverstripe-gridfieldextensions": "dev-master"
	},
	"require-dev": {
		"ccampbell/chromephp": "4.1.0"
	},
	"config": {
		"process-timeout": 1200
	},
	"scripts": {
		"post-install-cmd": "php ./framework/cli-script.php dev/build",
		"post-update-cmd": "php ./framework/cli-script.php dev/build"
	},
	"minimum-stability": "dev"
}
